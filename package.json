{
  "name": "moos",
  "version": "0.1.0",
  "description": "Moos Backend",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wolkeneis/Moos.git"
  },
  "author": "Frooastside",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/wolkeneis/Moos/issues"
  },
  "homepage": "https://moos.wolkeneis.dev/",
  "scripts": {
    "start": "node -r source-map-support/register dist/app.js",
    "restart": "yarn build && yarn start",
    "watch": "nodemon -e ts -L -w src --exec \"yarn restart\"",
    "lint": "eslint --ext ts .",
    "lint:fix": "yarn lint --fix",
    "pretty": "prettier --write .",
    "build": "tsc --incremental",
    "clean": "rm -rf dist",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/csurf": "^1.11.2",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/node": "^17.0.33",
    "@types/oauth2orize": "^1.8.10",
    "@types/passport": "^1.0.7",
    "@types/passport-discord": "^0.1.5",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-http": "^0.3.9",
    "@types/passport-http-bearer": "^1.0.37",
    "@types/passport-oauth2-client-password": "^0.1.2",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "nodemon": "^2.0.16",
    "prettier": "2.6.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --ext ts --cache --fix",
    "*.{js,ts,css,md,json}": "prettier --write"
  },
  "dependencies": {
    "argon2": "^0.28.5",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "firebase-admin": "^10.2.0",
    "moos-api": "^0.0.3",
    "oauth2orize": "^1.11.1",
    "passport": "^0.5.2",
    "passport-discord": "^0.1.4",
    "passport-google-oauth20": "^2.0.0",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "passport-oauth2-client-password": "^0.1.2",
    "source-map-support": "^0.5.21",
    "uuid": "^8.3.2"
  }
}
