{
  "name": "moos",
  "version": "0.1.0",
  "description": "Moos Backend",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wolkeneis/Moos.git"
  },
  "author": "Frooastside",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/wolkeneis/Moos/issues"
  },
  "homepage": "https://moos.wolkeneis.dev/",
  "scripts": {
    "prestart": "npm run build",
    "start": "node -r source-map-support/register dist/app.js",
    "watch": "nodemon -e ts -L -w src --exec \"npm run start\"",
    "lint": "eslint --ext ts src",
    "lint:fix": "npm run lint --fix",
    "pretty": "prettier --write .",
    "build": "npx tsc --incremental",
    "clean": "rm -rf dist",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.8",
    "nodemon": "^2.0.15",
    "prettier": "2.6.2",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --ext ts --cache --fix",
    "*.{js,ts,css,md,json}": "prettier --write"
  },
  "dependencies": {
    "express": "^4.17.3"
  }
}
